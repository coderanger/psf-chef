; Generated by Chef
; Local modifications will be overwritten

[database]
host = <%= @database[:host] %>
name = <%= @database[:name] %>
user = <%= @database[:user] %>
password = <%= @database[:password] %>

files_dir = <%= @dirs[:files] %>
docs_dir = <%= @dirs[:docs] %>

pubsubhubbub = <%= @pubsubhubbub %>


[webui]
debug_mode = <%= @debug %>

mailhost = <%= @mail[:host] %>
adminemail = <%= @mail[:email] %>
replyto = <%= @mail[:reply] %>

url = <%= @urls[:webui] %>
files_url = <%= @urls[:files] %>
package_docs_url = <%= @urls[:docs] %>
pydotorg = <%= @urls[:python] %>

simple_script = <%= @scripts[:simple] %>
simple_sign_script = <%= @scripts[:simple_sign] %>

key_dir = <%= @dirs[:key] %>

sshkeys_update = <%= @sshkeys_update %>

rss_file = <%= @rss[:pypi] %>
packages_rss_file = <%= @rss[:packages] %>

cheesecake_password = <%= @cheesecake_password %>
reset_secret = <%= @reset_secret %>


[passlib]
schemes = <%= @passlib[:schemes].join(", ") %>
deprecated = <%= @passlib[:deprecated].join(", ") %>


[logging]
file =
mailhost = <%= @mail[:host] %>
fromaddr = <%= @mail[:email] %>
toaddrs = <%= @admins.join(",") %>


[sentry]
dsn = <%= @sentry[:dsn] %>


[fastly]
api_domain = <%= @fastly[:api_domain] %>
api_key = <%= @fastly[:api_key] %>
service_id = <%= @fastly[:service_id] %>
